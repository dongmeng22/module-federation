{
  "name": "module-federation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm run install",
    "install": "npm-run-all --parallel install:* && pnpm run start",
    "install:host": "cd host && pnpm i",
    "install:remote": "cd remote && pnpm i",
    "start": "npm-run-all --parallel start:*",
    "start:host": "cd host && pnpm run dev",
    "start:remote": "cd remote && pnpm run dev",
    "build": "npm-run-all --parallel build:*",
    "build:host": "cd host && pnpm run build",
    "build:remote": "cd remote && pnpm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
